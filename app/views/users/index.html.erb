
<%= render 'books/list', book: @book, user: current_user %>
<h1>Users</h1>

  <table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th></th>
    </tr>
  </thead>
  
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= link_to user_path(user.id) do %>
          <%= image_tag user.get_profile_image(100,100) %>
          <% end %>
      </td>
       <td><%= user.name %></td>
       <td><%= link_to "Show",user_path(user.id) %></td>
    </tr>
  <% end %>
  </tbody>
  </table>